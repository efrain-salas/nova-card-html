{
    "name": "efrain-salas/nova-card-html",
    "description": "Simple Nova Card to display HTML",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "EfrainSalas\\NovaCardHtml\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EfrainSalas\\NovaCardHtml\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
